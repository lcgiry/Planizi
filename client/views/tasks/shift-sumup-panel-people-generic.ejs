<div class="col-xs-12 col-sm-6 col-md-4 col-lg-3">
    <div class="panel panel-full-dark xs-mb-5">
        <div class="panel-body text-center">
            <div class="font-20px hidden-xs hidden-sm visible-md-block visible-lg-block" style="line-height: 34px;">
                <span class="nb-pepople"><%= number %></span>&nbsp;<i class="fas fa-male"></i>&nbsp;&nbsp;
                <% skills.forEach((skill, index) => { %>
                    <% if (skill != ''){ %>
                        <span class="label label-success" style="white-space: normal;"><%= skill %></span>
                    <% } %>
                <% }); %>
                <% if (team != '' && team != 0){ %>
                    <span class="label label-info" style="white-space: normal;"><%= team %></span>
                <% } %>
                <% if (trust != '' && trust != 0){ %>
                    <span class="label label-warning" style="white-space: normal;"><%= trust %>/10</span>
                <% } %>
            </div>

            <div class="font-16px visible-xs visible-sm-block hidden-md" style="line-height: 28px;">
                <span class="nb-pepople"><%= number %></span>&nbsp;<i class="fas fa-male"></i>&nbsp;&nbsp;
                <% skills.forEach((skill, index) => { %>
                <% if (skill != ''){ %>
                <span class="label label-success" style="white-space: normal;"><%= skill %></span>
                <% } %>
                <% }); %>
                <% if (team != '' && team != 0){ %>
                <span class="label label-info" style="white-space: normal;"><%= team %></span>
                <% } %>
                <% if (trust != '' && trust != 0){ %>
                <span class="label label-warning" style="white-space: normal;"><%= trust %>/10</span>
                <% } %>
            </div>
        </div>
        <div class="panel-footer font-16px xs-p-5 text-center">
            <span class="id-shift-sumup font-color-black">0001-001</span>&nbsp;
            <button class="btn btn-info"><i class="fas fa-edit"></i></button>&nbsp;
            <button class="btn btn-danger"><i class="fas fa-trash"></i></button>
        </div>
    </div>
</div>